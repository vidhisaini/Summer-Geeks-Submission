<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <link rel="apple-touch-icon" sizes="76x76" href="/static/img/favicon.ico">
  <title>Innovaccer Entry Management System</title>
  <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
  <meta name="viewport" content="width=device-width" />

  <link rel="icon" type="image/png" href="./img/favicon.png" />

  <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />

  <link href="./css/bootstrap.min.css" rel="stylesheet" />
  <link href="./css/material-bootstrap-wizard.css" rel="stylesheet" />

</head>

<body>
  <script src="./js/jquery-2.2.4.min.js" type="text/javascript"></script>
  <script src="./js/material-bootstrap-wizard.js"></script>

  <script src="./js/bootstrap.min.js" type="text/javascript"></script>
  <script src="./js/jquery.bootstrap.js" type="text/javascript"></script>
  <script src="./js/jquery.validate.min.js"></script>

  <div class="image-container set-full-height" style="background-image: url('./img/basic.jpg')">

    <div class="container">
      <div class="row">
        <div class="col-sm-8 col-sm-offset-2" style="margin-top:-3%;">
          <div class="wizard-container">
            <div class="card wizard-card" data-color="green" id="wizardProfile">
              <form method="POST">
                <div class="wizard-header">
                  <h3 class="wizard-title">
                    Summer Geeks
                  </h3>
                  <h5>Please fill in details about your visit</h5>
                </div>
                <div class="wizard-navigation">
                  <ul>
                    <li><a id="abt" href="#about" data-toggle="tab">Your Details</a></li>
                    <li><a id="hd" href="#address" data-toggle="tab">Host Details</a></li>
                  </ul>
                </div>
                <div class="tab-content">
                  <div class="tab-pane" id="about">
                    <div class="row">
                      <div class="col-sm-5 col-sm-offset-1">
                        <div class="input-group">
                          <span class="input-group-addon">
                            <i class="material-icons">face</i>
                          </span>
                          <div class="form-group label-floating">
                            <label id="fname" class="control-label">Name </label>
                            <input name="visitor_name" type="text" class="form-control">
                          </div>
                        </div>
                      </div>
                      <div class="col-sm-5">
                        <div class="input-group">
                          <span class="input-group-addon">
                            <i class="material-icons">local_phone</i>
                          </span>
                          <div class="form-group label-floating">
                            <label class="control-label">Phone Number </label>
                            <input name="visitor_phone_number" type="text" class="form-control">
                          </div>
                        </div>
                      </div>
                      <div class="col-sm-10 col-sm-offset-1">
                        <div class="input-group">
                          <span class="input-group-addon">
                            <i class="material-icons">email</i>
                          </span>
                          <div class="form-group label-floating">
                            <label class="control-label">Email </label>
                            <input name="visitor_email" type="email" class="form-control">
                          </div>
                        </div>
                      </div>
                      <div class="col-sm-10 col-sm-offset-1">
                        <div class="input-group">
                          <span class="input-group-addon">
                            <i class="material-icons">near_me</i>
                          </span>
                          <div class="form-group label-floating">
                            <label class="control-label">Visiting Address </label>
                            <input name="host_address" type="text" class="form-control">
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="tab-pane" id="address">
                    <div class="row">
                      <div class="row">
                        <div class="col-sm-10 col-sm-offset-1">
                          <div class="input-group is-invalid">
                            <span class="input-group-addon">
                              <i class="material-icons">face</i>
                            </span>
                            <div class="form-group label-floating">
                              <label class="control-label">Name </label>
                              <input name="host_name" type="text" class="form-control">
                            </div>
                          </div>
                        </div>
                        <div class="col-sm-10 col-sm-offset-1">
                          <div class="input-group">
                            <span class="input-group-addon">
                              <i class="material-icons">email</i>
                            </span>
                            <div class="form-group label-floating">
                              <label class="control-label">Email </label>
                              <input name="host_email" type="email" class="form-control">
                            </div>
                          </div>
                        </div>
                        <div class="col-sm-10 col-sm-offset-1">
                          <div class="input-group">
                            <span class="input-group-addon">
                              <i class="material-icons">local_phone</i>
                            </span>
                            <div class="form-group label-floating">
                              <label class="control-label">Phone Number </label>
                              <input name="host_phone_number" type="text" class="form-control">
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="wizard-footer" style="margin-top:2%;">
                    <div class="pull-right">
                      <input id="next" type='button' class='btn btn-next btn-fill btn-success btn-wd' name='next' value='Next' />
                      <input id="finish"  class='btn btn-finish btn-fill btn-success btn-wd' type="submit" name='button' value='Finish' />
                    </div>

                    <div class="pull-left">
                      <input id="previous" type='button' class='btn btn-previous btn-fill btn-default btn-wd' name='previous' value='Previous' />
                    </div>
                    <div class="clearfix"></div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
      <script src="./js/main.js"></script>
    </body>
  </html>
